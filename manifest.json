  "shortcuts": [
    {
      "name": "Quadratic Calculator",
      "short_name": "Quadratic",
      "description": "Solve quadratic equations",
      "url": "/algebra/quadratic-equations/",
      "icons": [
        {
          "src": "/icons/quadratic-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Derivative Calculator",
      "short_name": "Derivative",
      "description": "Calculate derivatives",
      "url": "/calculus/derivative-calculator/",
      "icons": [
        {
          "src": "/icons/derivative-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "AI Math Assistant",
      "short_name": "AI Assistant",
      "description": "Get help from AI",
      "url": "/ai-assistant.html",
      "icons": [
        {
          "src": "/icons/ai-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [],
  "features": [
    "math-keyboard",
    "offline-access",
    "push-notifications",
    "background-sync"
  ],
  "share_target": {
    "action": "/share/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "image",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "math",
      "url": "/solve?equation=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/open",
      "accept": {
        "text/plain": [".math", ".eq"],
        "application/json": [".mathdata"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": ["focus-existing", "auto"]
  },
  "handle_links": "preferred",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "edge_side_panel": {
    "preferred_width": 400
  }
}